.seta {
    width: 17px;
    height: 30px;
}

.seta:hover,
.pagina:hover {
    cursor: pointer;
}

.seta-esquerda {
    background-image: url("../imagens/SVG/setaesquerda1.svg");
}

.seta-esquerda:hover {
    background-image: url("../imagens/SVG/setaesquerda2.svg");
}

.seta-direita {
    background-image: url("../imagens/SVG/setadireita1.svg");
}

.seta-direita:hover {
    background-image: url("../imagens/SVG/setadireita2.svg");
}

.pagina{
    font-family: 'IBM Plex Sans'  !important;
    font-weight: 500  !important;
    font-size: 23px  !important;
    color: var(--cinza-4d);
}
.pagina:hover {
    color: var(--dourado-b1) !important;
}
.ativa{
    color: var(--dourado-b1);
}
.voltar {
    border: none;
    width: 120px !important;
    height: 30px !important;
    background-image: url(../imagens/SVG/voltar1.svg);
    background-repeat: no-repeat;
    background-position: center;
    background-size: cover;
}

.voltar:hover {
    background-image: url(../imagens/SVG/voltar2.svg);
}
.data-publicacao {
    font-family: 'IBM Plex Sans'  !important;
    font-weight: 500  !important;
    font-size: 13px  !important;
    text-transform: uppercase;
    color: var(--cinza-4d);
}
.imprensa .divisoria{
    border: 1px solid var(--dourado-a9);
}
.separa-noticias{
    height: 10px;
    /* border: 1px solid black; */
    box-shadow: 0px 4px 6px -2px rgba(128, 128, 128, 0.5);
}